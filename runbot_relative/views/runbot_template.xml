  <template id="build_button" inherit_id="runbot.build_button" name="Build Button Protocol Relative">
      <xpath expr="." position="inside">
          <input type="hidden" id="runbot-host" t-att-value="bu['host']" />
          <input type="hidden" id="runbot-domain" t-att-value="bu['domain']" />
      </xpath>
  </template>

</odoo>

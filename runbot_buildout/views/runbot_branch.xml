<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_runbot_branch_form" model="ir.ui.view">
            <field name="model">runbot.branch</field>
            <field name="inherit_id" ref="runbot.view_branch_form" />
            <field name="arch" type="xml">
                <field name="sticky" position="after">
                    <field name="buildout_default" attrs="{'invisible': [('buildout_version', '=', False)]}" />
                </field>
                <field name="pull_head_name" position="after">
                    <field name="buildout_version" attrs="{'invisible': [('buildout_version', '=', False)]}" />
                </field>
            </field>
        </record>
    </data>
</openerp>
